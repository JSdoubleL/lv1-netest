#NEXUS
begin data;
dimensions ntax=5 nchar=24;
format datatype = standard gap = - missing = ? symbols = " 0 1";
matrix
taxon_1 100000000000000000000000
taxon_2 110001000000000000000000
taxon_3 111001000000000000000000
taxon_5 100110100000000000000000
taxon_6 111110000000000000000000
;
end;

begin assumptions;
	options deftype=unord;
 end;
 
begin paup;
	set criterion=parsimony maxtrees=100 increase=no;
	hsearch start=stepwise addseq=random nreps=25 swap=tbr collapse=no;
	filter best=yes;
	describetrees 1/diag=yes;
	pscores all/ ci ri rc hi scorefile=polytomy_0_3_scores.tsv replace=yes;
	savetrees file=polytomy_0_3_trees.nex replace=yes format=nexus;
	quit;
end;